{
  "server": {
    "port": 8080,
    "host": "0.0.0.0"
  },
  "docker": {
    "executablePath": "/usr/bin/docker",
    "environment": {
      "DOCKER_HOST": "unix:///var/run/docker.sock"
    }
  },
  "deployment": {
    "name": "myapp",
    "dockerfile": "../myapp/Dockerfile",
    "context": "../myapp",
    "replicas": 3,
    "containerPort": 8080,
    "healthCheckPath": "/health",
    "healthCheckInterval": 10,
    "remoteUrl": "https://github.com/owner/myapp"
  }
}
